{
  "version": 2,
  "builds": [
    {
      "src": "apps/nextjs-blog/next.config.mjs",
      "use": "@vercel/next"
    },
    {
      "src": "apps/node-api/src/app.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/python-api/app/__init__.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/node-api/src/app.ts"
    },
    {
      "src": "/python-api/(.*)",
      "dest": "/apps/python-api/app/__init__.py"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/nextjs-blog/next.config.mjs"
    }
  ]
}
